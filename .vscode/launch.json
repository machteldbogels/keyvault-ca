    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch KeyVaultCA",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/KeyVaultCA/bin/Debug/net6.0/KeyVaultCA.dll",
            "args": ["${workspaceFolder}/appsettings.json"],
            "cwd": "${workspaceFolder}/KeyVaultCA",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Launch KeyVaultCA.Web",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/KeyVaultCA.Web/bin/Debug/net6.0/KeyVaultCA.Web.dll",
            "args": ["${workspaceFolder}/appsettings.json"],
            "cwd": "${workspaceFolder}/KeyVaultCA.Web",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Launch E2ETest",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/DeviceProvisioningE2ETest/bin/Debug/net6.0/E2ETests.dll",
            "args": ["${workspaceFolder}/appsettings.json"],
            "cwd": "${workspaceFolder}/DeviceProvisioningE2ETest",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}